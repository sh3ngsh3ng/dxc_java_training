<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="Student" table="STUD_HIBERNATE">
		<id name="id" type="int" column="ID"></id>
		<property name="name">
			<column name="NAME" />
		</property>
	</class>
</hibernate-mapping>